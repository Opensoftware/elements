<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>{{ title }}</title>
    {% include 'partials/favicon.njk' %}
    <!-- Menu -->
    <link rel="stylesheet" href="/css/menu/ie5.css">
    <link rel="stylesheet" href="/css/menu/ie7.css">
    <!--[if IE 5]>
    <link rel="stylesheet" href="/css/menu/ie5-fix.css">
    <![endif]-->
    <link rel="stylesheet" href="/css/menu/ie8.css">
    <link rel="stylesheet" href="/css/menu/mobile.css">
    <link rel="stylesheet" href="/css/menu/flex.css">

    <script>
      (function() {
        var html = document.getElementsByTagName('html')[0];

        if(html) {
          html.className += html.className ? " js" : "js";
        }

        if (typeof SVGRect !== "undefined") {
          html.className +=html.className ? " svg" : "svg";
          window.svg = true;
        }
      })();
    </script>
  </head>
  <body>
    {% include 'partials/header.njk' %}
    <div class="container">
      <main>
        {{ content | safe }}
      </main>
    </div>
  </body>
  <!-- Header -->
  <script src="/js/utils/class-function.js"></script>
  <script src="/js/utils/window-sizes.js"></script>
